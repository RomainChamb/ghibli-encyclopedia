{
  "name": "seed4j-sample-application",
  "version": "0.0.0",
  "private": true,
  "description": "Seed4J Sample Application",
  "license": "UNLICENSED",
  "packageManager": "npm@11.6.2",
  "jestSonar": {
    "reportPath": "target/test-results",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:ng": "ng serve",
    "ng": "ng",
    "start": "ng serve",
    "build": "npm-run-all build:*",
    "build:ng": "ng build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ng": "ng build --watch --configuration development",
    "test": "npm run watch:test",
    "test:coverage": "ng test --coverage",
    "watch:test": "ng test --watch",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "9.38.0",
    "@angular-builders/jest": "20.0.0",
    "@angular/build": "20.3.8",
    "@angular/cli": "20.3.8",
    "@angular/compiler-cli": "20.3.9",
    "@types/node": "24.9.2",
    "@types/jest": "29.5.14",
    "angular-eslint": "20.5.0",
    "globals": "16.4.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.4.5",
    "jest-preset-angular": "14.6.2",
    "jest-sonar-reporter": "2.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "npm-run-all2": "8.0.4",
    "@prettier/plugin-xml": "3.4.2",
    "prettier": "3.6.2",
    "prettier-plugin-gherkin": "3.1.3",
    "prettier-plugin-java": "2.7.7",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-packagejson": "2.5.19",
    "husky": "9.1.7",
    "lint-staged": "16.2.6"
  },
  "dependencies": {
    "@angular/animations": "20.3.9",
    "@angular/cdk": "20.2.11",
    "@angular/common": "20.3.9",
    "@angular/compiler": "20.3.9",
    "@angular/core": "20.3.9",
    "@angular/material": "20.2.11",
    "@angular/forms": "20.3.9",
    "@angular/platform-browser": "20.3.9",
    "@angular/platform-browser-dynamic": "20.3.9",
    "@angular/router": "20.3.9",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  }
}
